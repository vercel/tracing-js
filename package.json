{
  "name": "@zeit/tracing-js",
  "version": "0.4.0",
  "description": "A partial implementation of the Opentracing JavaScript API for honeycomb.io backend",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "fmt": "prettier --single-quote --bracket-spacing --trailing-comma all --write './{src,test,types,examples}/**/*.ts'",
    "test": "tape dist/test/**.js",
    "codecov": "c8 --check-coverage --lines 85 node dist/test/setup-http.test.js && codecov"
  },
  "repository": "github:zeit/tracing-js",
  "author": "styfle",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@types/node": "^11.9.4",
    "@types/node-fetch": "^2.1.6",
    "@types/tape": "^4.2.33",
    "c8": "^3.4.0",
    "codecov": "^3.2.0",
    "node-fetch": "^2.3.0",
    "prettier": "^1.16.4",
    "tape": "^4.10.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "libhoney": "^1.2.1"
  },
  "peerDependencies": {
    "node-fetch": "*"
  }
}
